#include <stdio.h>

int main() {
    int T[20] , n , i ,min;
    printf("donnez la taille  \n");
    scanf("%d",&n);
    
  for(i=0;i<n;i++)
  {
      printf("donnez un nombre \n ");
      scanf("%d",&T[i]);
  }
  min=T[0];
  for(i=0;i<n;i++)
  {
      if(min>T[i])
      {
          min=T[i];
      }
  }
  printf("le min est %d \n ",min);
  
  

    return 0;
}